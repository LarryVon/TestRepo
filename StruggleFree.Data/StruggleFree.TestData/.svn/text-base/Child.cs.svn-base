using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using StruggleFree.Data;

namespace TestHarness
{
    [Serializable()]
    public class Child : ChildEntityData
    {
        public Child()
            : base() { }

        public string Name
        {
            get
            {
                return EditValue;
            }
            set
            {
                EditValue = value;
            }
        }

        public int ParentID
        {
            get
            {
                return ParentInstanceID;
            }
            set
            {
                ParentInstanceID = value;
            }
        }
    }
}
